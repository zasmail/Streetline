<% @group.spaces.where(group_id: @group.id).each do |space| %>
  <p>
    <strong>ID:</strong>
    <%= space.unique_id %>
    <strong> Group ID: </strong>
    <%= space.group_id %>
   <%= link_to 'Destroy Space', [space.group, space],
               method: :delete,
               data: { confirm: 'Are you sure?' } %>
  </p>
<% end %>
